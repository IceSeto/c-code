//#include <stdio.h>
//#define MAX 100//#define 定义的标识符常量
//#define可以定义宏--带参数
int MAX1(int x, int y)
{
	if (x > y)
		return x;
	else
		return y;

}
//宏的定义
#define MAX2(X,Y) X>Y?X:Y;
int main()
{
	int a = 10;
	int b = 20;
	//函数方式比大小
	int max1 = MAX1 (a, b);
	printf("%d\n", max1);
	//宏方式
	int max = MAX2 (a, b);
	//max = (a>b?a:b;)
	printf("%d\n", max);

	return 0;
}

