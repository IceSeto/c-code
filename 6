int main()
{
	//(2进制位)
	//&按位与
	//|按位或
	//^按位异或
	int a = 3;
	int b = 5;
	int c = a & b;
	int d = a | b;
	int e = a ^ b;
	printf("%d\n", c);
	printf("%d\n", d);
	printf("%d\n", e);//离散数学


	return 0;
}
