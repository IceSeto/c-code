#include <stdio.h>
#include <string.h>
int main()
{
	printf("%c\n", '\32');
	printf("%c\n", '\32');
	printf("%c\n", '\x61');
	printf("%d\n", strlen("c:\test\32\test.c"));
	printf("%d\n", strlen("c:\test\382\test.c"));
	return 0;
}
